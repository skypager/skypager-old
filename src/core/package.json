{
  "name": "skypager-core",
  "version": "38.4.0",
  "description": "a javscript launch platform",
  "main": "index.js",
  "repository": {
    "type": "github",
    "url": "https://github.com/skypager/skypager"
  },
  "author": "Jon Soeder <jon@chicago.com> https://github.com/datapimp",
  "license": "MIT",
  "dependencies": {
    "skypager-helpers-client": "^38.4.0",
    "skypager-helpers-command": "^38.4.0",
    "skypager-helpers-document": "^38.4.0",
    "skypager-helpers-document-type": "^38.4.0",
    "skypager-helpers-page": "^38.4.0",
    "skypager-helpers-project": "^37.5.0",
    "skypager-helpers-project-type": "^37.5.0",
    "skypager-helpers-repl": "^38.4.0",
    "skypager-helpers-server": "^38.4.0",
    "skypager-helpers-service": "^38.4.0",
    "skypager-runtime": "^38.4.0"
  },
  "homepage": "https://skypager.io",
  "compilers": {
    "package": {
      "entryPoints": "index.js"
    },
    "minifiedBundle": {
      "target": "web",
      "minify": true,
      "externals": {
        "skypager-runtime": "global skypager"
      },
      "entryPoints": {
        "skypager-core": [
          "expose?skypager!skypager-runtime/skypager-runtime.js",
          "index.web.js"
        ]
      }
    },
    "bundle": {
      "target": "web",
      "externals": {
        "skypager-runtime": "global skypager"
      },
      "entryPoints": {
        "skypager-core": [
          "expose?skypager!skypager-runtime/skypager-runtime.js",
          "index.web.js"
        ]
      }
    }
  }
}
