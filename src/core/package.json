{
  "name": "skypager-core",
  "version": "39.7.0",
  "description": "a javscript launch platform",
  "main": "index.js",
  "repository": {
    "type": "github",
    "url": "https://github.com/skypager/skypager"
  },
  "author": "Jon Soeder <jon@chicago.com> https://github.com/datapimp",
  "license": "MIT",
  "dependencies": {
    "skypager-helpers-client": "^39.7.0",
    "skypager-helpers-command": "^39.7.0",
    "skypager-helpers-document": "^39.7.0",
    "skypager-helpers-document-type": "^39.7.0",
    "skypager-helpers-page": "^39.7.0",
    "skypager-helpers-project": "^39.6.1",
    "skypager-helpers-project-type": "^39.6.1",
    "skypager-helpers-repl": "^39.7.0",
    "skypager-helpers-server": "^39.7.0",
    "skypager-helpers-service": "^39.7.0",
    "skypager-runtime": "^39.7.0"
  },
  "homepage": "https://skypager.io",
  "compilers": {
    "package": {
      "entryPoints": "index.js"
    },
    "minifiedBundle": {
      "target": "web",
      "minify": true,
      "externals": {
        "skypager-runtime": "global skypager"
      },
      "entryPoints": {
        "skypager-core": [
          "expose?skypager!skypager-runtime/skypager-runtime.js",
          "index.web.js"
        ]
      }
    },
    "bundle": {
      "target": "web",
      "externals": {
        "skypager-runtime": "global skypager"
      },
      "entryPoints": {
        "skypager-core": [
          "expose?skypager!skypager-runtime/skypager-runtime.js",
          "index.web.js"
        ]
      }
    }
  }
}
